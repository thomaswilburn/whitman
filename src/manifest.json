{
  "name": "Whitman",
  "description": "A simple sampler for Chrome and Chrome OS",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "assets/icon.jpg"
  },
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "syncFileSystem",
    {
      "fileSystem": [
        "write"
      ]
    },
    "http://maxcdn.bootstrapcdn.com/*",
    "http://fonts.googleapis.com/*"
  ],
  "offline_enabled": true,
  "file_handlers": {
    "text": {
      "extensions": [
        "jsong"
      ]
    }
  }
}
